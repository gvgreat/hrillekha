<?xml version="1.0" encoding="UTF-8"?>
<crown-report xmlns="http://www.techlords.com/crown-report"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.techlords.com/crown-report crown-report.xsd ">
	<bean-name>CrownAuditBO</bean-name>
	<title>Audit Report</title>
	<sub-title>Audit Report for all Entities</sub-title>
	<margin>20</margin>
	<detail-height>15</detail-height>
	<column-params>
		<column-name>tableName</column-name>
		<column-title>Entity Name</column-title>
		<class-name>java.lang.String</class-name>
		<column-width>85</column-width>
		<criteria-column>true</criteria-column>
	</column-params>
	<column-params>
		<column-name>crownUser</column-name>
		<column-title>User</column-title>
		<class-name>java.lang.String</class-name>
		<column-width>85</column-width>
	</column-params>
	<column-params>
		<column-name>action</column-name>
		<column-title>Action</column-title>
		<class-name>java.lang.String</class-name>
		<column-width>85</column-width>
	</column-params>
	<column-params>
		<column-name>description</column-name>
		<column-title>Description</column-title>
		<class-name>java.lang.String</class-name>
		<column-width>85</column-width>
	</column-params>
	<column-params>
		<column-name>timestamp</column-name>
		<column-title>Time Stamp</column-title>
		<class-name>java.lang.String</class-name>
		<column-width>85</column-width>
	</column-params>
</crown-report>
